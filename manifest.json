{
  "manifest_version": 2,

  "name": "YouTube Louder",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "extension.js"
      ],
      "matches": ["https://*.youtube.com/*"]
    }
  ],

  "permissions": [
    "activeTab",
    "storage"
  ]
}
